.nav-bar-page {
  width: 100%;
  height: 100%;
}

.nav-bar {
  display: flex;
  align-items: center;
  width: 700rpx;
  /* text-align: left; */
  font-size: 32rpx;
  font-weight: 600;
  box-sizing: border-box;
  line-height: 44px;
  position: fixed;
  top: 15rpx;
  left: 35rpx;
  z-index: 9;
}

.reportTitle {
  margin-left: 30%;
  font-weight: bold;
  font-size: 40rpx;
  color: #222222;
  line-height: 60rpx;
  text-align: left;
  font-style: normal;
}

.nav-bar image {
  width: 42rpx;
  height: 40rpx;
}

.procedureEnd {
  width: 100vw;
  height: 100%;
  background: rgba(0, 0, 0, 0.77);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99999;
}

.popup-box {
  width: 602rpx;
  height: 492rpx;
  background-image: url(https://zskjobject.oss-cn-beijing.aliyuncs.com/zskj_nbj_mini/img/sc-model.png);
  background-size: 100%;
  background-repeat: no-repeat;
  position: absolute;
  top: 30%;
  left: 70rpx;
  z-index: 99999;
}

.title {
  display: flex;
  justify-content: center;
  font-size: 32rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #222222;
  margin: 16rpx 0;
}

.content {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx 40rpx;
  margin-top: 100rpx;
}

.btn-box {
  display: flex;
  justify-content: space-between;
  margin-top: 100rpx;
  padding: 0 20rpx;
}

.confim {
  width: 260rpx;
  height: 76rpx;
  background: #3476FF;
  border-radius: 16rpx;
  line-height: 76rpx;
  font-size: 28rpx;
  font-family: PingFang-SC, PingFang-SC;
  font-weight: bold;
  color: #FFFFFF;
  text-align: center;
}

.err-box,
.tip-box {
  width: 100%;
  height: 100%;
  background-color: #000000;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99999;
}

.err-bj-i {
  width: 638rpx;
  height: 888rpx;
  position: absolute;
  top: 190rpx;
  left: 56rpx;
  z-index: 999;
}

.close {
  width: 228rpx;
  height: 100rpx;
  position: absolute;
  top: 940rpx;
  left: 88rpx;
  z-index: 999;
}

.anew {
  width: 340rpx;
  height: 100rpx;
  position: absolute;
  top: 940rpx;
  left: 330rpx;
  z-index: 999;
}
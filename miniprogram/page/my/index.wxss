.addinfodialog {
  position: relative;
}
.placeholderStyle { 
  color: #333; 
}
.addinfobg {
  width: 582rpx;
  height: 650rpx;
}

.addinfobut-box {
  height: 0;
  position: relative;
}

.addinfobut {
  position: absolute;
  top: -164rpx;
  left: 44rpx;
  width: 498rpx;
  height: 122rpx;
}

.deleteItem {
  width: 50rpx;
  height: 50rpx;
}

.deleteicon {
  position: absolute;
  top: 14rpx;
  left: 18rpx;
  width: 50rpx;
  height: 50rpx;
}

.info {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 18rpx 20rpx 26rpx 36rpx;
  box-sizing: border-box;
}

.in-left {
  display: flex;
}

.img,
.bg {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.name-box {
  display: flex;
  align-items: center;
  margin-left: 24rpx;
}

.nick-name {
  font-size: 40rpx;
  font-family: PingFang-SC, PingFang-SC;
  font-weight: bold;
  color: #222222;
  display: flex;
  align-items: center;
}

.nick-name image {
  width: 52rpx;
  height: 52rpx;
}



.bot {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.sysbtn {
  width: 120rpx;
  height: 48rpx;
  background: #3476FF;
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.09);
  border-radius: 28rpx;
  border: 1rpx solid #FFFFFF;
  font-size: 22rpx;
  font-family: SourceHanSansCN, SourceHanSansCN;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 48rpx;
  text-align: center;
}

.edit {
  width: 152rpx;
  height: 48rpx;
  background: #FF9334;
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.09);
  border-radius: 28rpx;
  border: 1rpx solid #FFFFFF;
  font-size: 22rpx;
  font-family: SourceHanSansCN, SourceHanSansCN;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 20rpx;
}

.edit image {
  width: 22rpx;
  height: 20rpx;
}

.edit-title {
  font-size: 22rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  text-align: center;
}

.editbg {
  width: 22rpx;
  height: 20rpx;
  margin-right: 8rpx;
}

.person {
  width: 152rpx;
  height: 48rpx;
  background: #11D127;
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.09);
  border-radius: 28rpx;
  border: 1rpx solid #FFFFFF;
  font-size: 22rpx;
  font-family: SourceHanSansCN, SourceHanSansCN;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.person image {
  width: 22rpx;
  height: 20rpx;
}

.person text {
  font-size: 22rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
}

.serve {
  width: 694rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 38rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 20rpx;
  margin: 0 auto;
  padding: 22rpx 34rpx 32rpx;
  box-sizing: border-box;
}

.serve-title {
  font-size: 32rpx;
  font-family: PingFang-SC, PingFang-SC;
  font-weight: bold;
  color: #222222;
}

.serve-list {
  display: flex;
  justify-content: space-around;
  margin-top: 16rpx;
}

.serve-one {
  width: fit-content;
  height: fit-content;
  text-align: center;
}

.servebg {
  width: 98rpx;
  height: 100rpx;
}

.serve-text {
  font-size: 24rpx;
  font-family: PingFang-SC, PingFang-SC;
  font-weight: 500;
  color: #222222;
}

.caide {
  width: 100%;
  height: 214rpx;
  margin: 20rpx 0;
}
.btn {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  bottom: 62rpx;
}

.no {
  font-size: 26rpx;
  font-weight: bold;
  color: #696969;
  line-height: 36rpx;
}

.button {
  width: 686rpx;
  height: 98rpx;
  margin-top: 24rpx;
  background: #3476FF;
  border-radius: 49rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}

.equity-center {
  width: 646rpx;
  height: 48rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 38rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 20rpx;
  padding: 24rpx 20rpx;
  margin: 24rpx auto 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.equity-left {
  display: flex;
  align-items: center;
}

.equity-left view {
  font-size: 28rpx;
  font-family: PingFang-SC, PingFang-SC;
  font-weight: bold;
  color: #222222;
  line-height: 40rpx;
}

.equity-right {
  display: flex;
  align-items: center;
}

.equity-img:nth-child(1) {
  width: 40rpx;
  height: 40rpx;
  margin-right: 8rpx;
}

.equity-img:nth-child(2) {
  width: 32rpx;
  height: 32rpx;
}

.model {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(16, 16, 16, 0.69);
  z-index: 9999;
}

.model-info {
  width: 602rpx;
  height: 522rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 400rpx;
  left: 65rpx;
}

.title {
  margin: 36rpx auto;
  text-align: center;
}

.content {
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding: 20rpx 34rpx;
  font-size: 26rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #656565;
}



.content-input {
  width: 370rpx;
  height: 76rpx;
  padding: 0 15rpx;
  background: rgba(216, 216, 216, 0.19);
  border-radius: 16rpx;
  opacity: 0.72;
  border: 2rpx solid rgba(151, 151, 151, 0.24);
  display: flex;
  align-items: center;
}

.password-icon {
  width: 40rpx;
  height: 40rpx;
}

.btn-box {
  display: flex;
  justify-content: center;
  margin-top: 45rpx;
  padding: 0 20rpx;
}

input[type=text] {
  color: #222;
}

input[type=password] {
  color: #222;
}

.clean {
  width: 246rpx;

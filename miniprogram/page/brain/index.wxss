  line-height: 50rpx;
}

.game-text {
  font-size: 22rpx;
  font-weight: 400;
  color: #676767;
  line-height: 32rpx;
  margin: 14rpx 0 18rpx 0;
}

.game-item {
  display: flex;
  margin-bottom: 30rpx;
}

.game-image {
  width: 216rpx;
  height: 200rpx;
  border-radius: 16rpx;
}

.game-content {
  width: 442rpx;
  margin-left: 24rpx;
}

.game-content-title {
  font-size: 28rpx;
  font-weight: bold;
  color: #222222;
  line-height: 40rpx;
  -webkit-line-clamp: 1;
  /* 文本位置只保留两行 */
  text-overflow: ellipsis;
  /* 溢出文本省略号代替 */
  display: -webkit-box;
  /*  */
  -webkit-box-orient: vertical;
  /*  */
  overflow: hidden;
  /* 溢出隐藏 */
}

.game-content-text {
  height: 78rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #666666;
  line-height: 28rpx;
  -webkit-line-clamp: 3;
  /* 文本位置只保留两行 */
  text-overflow: ellipsis;
  /* 溢出文本省略号代替 */
  display: -webkit-box;
  /*  */
  -webkit-box-orient: vertical;
  /*  */
  overflow: hidden;
  /* 溢出隐藏 */
  margin: 6rpx 0 18rpx 0;
}

.game-but-box {

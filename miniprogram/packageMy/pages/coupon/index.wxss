.page{
  width: 100%;
  background: #F3F4F6;
}
.scroll-view_H{
  white-space: nowrap;
  width:686rpx;
  height:50rpx;
  margin-bottom: 20rpx;
}
.scroll-view-item_H{
  display: inline-block;
}
.load{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 582rpx;
  width: 100%;
  margin-top: 15vh;
}
.coupon{
font-size: 28rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #9EA1A7;
line-height: 40rpx;
}
.tab{
  width: 170rpx;
  height: fit-content;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.tab-text{
  font-size: 28rpx;
  font-weight: 400;
  color: #464646;
  line-height: 40rpx;
}
.tab-text-active{
  font-size: 28rpx;
  font-weight: bold;
  color: #3476FF;
  line-height: 40rpx;
}
.tab-label{
  width: 28rpx;
  height: 4rpx !important;
  background: #3476FF;
  border-radius: 3rpx;
}
.search{
  width:748rpx;
  height:130rpx;
  background:#fff;
  border:1px solid #fff;
}
.search-box{
  width: 686rpx;
  height: 64rpx;
  background: #F3F4F6;
  border-radius: 32rpx;
  margin:30rpx auto;
  border:1px solid #F3F4F6;
  display: flex;
}
.search-icon{
  width:38rpx;
  height:38rpx;
  margin:15rpx 0rpx 0rpx 30rpx;
}
.search-input{
  width:600rpx;
  height:40rpx;
  line-height: 40rpx;
  font-size: 14px;
  color:#999;
  margin:13rpx;
}
.top{
  width:680rpx;
  height:fit-content;
}
.van-tab{
  flex-basis: auto !important;
}
.title{
  height:70rpx;
  font-size: 18px;
  font-weight: bold;
  color:#222;
}
.tabs{
  width:686rpx;
  height:50rpx;
  margin:auto;
  display: flex;
}
.van-tabs__line{
  width: 28rpx !important;
  border-radius: 3rpx !important;
  position: relative;
  top: 65rpx;
}
.van-tabs__scroll{
  background-color: #F3F4F6 !important;
}
.active{
  color:#3476FF;
  font-weight: bold;
}
.line{
  width:50rpx;
  height:5rpx;
  background-color: #3476FF !important;
  margin-left:20rpx;
}
.default{
  width:50rpx;
  height:5rpx;
  background-color: #F3F4F6;
}
.subscribe-map{
  width:686rpx;
  height:630rpx;
  margin: 20rpx auto;
}
.map{
  width:692rpx;
  height:fit-content;
  /* padding: 24rpx; */
 position: relative;
  /* background-image:url(https://neuroweave.oss-cn-hangzhou.aliyuncs.com/zskj-app-test/games/images/passzhe.png); */
  background-size: 100% 100%;
  margin:0 auto 24rpx;
  border-radius: 10px;
  box-sizing: border-box;
  padding-bottom: 10rpx;
}
.addres-map{
  width: 691rpx;
  box-sizing: border-box;
}
.map-title{
  font-size: 16px;
  color:#222;
  font-weight: bold;
}
.map-text{
  overflow: hidden;
}
.map-box{
  width: 636rpx;
  height: 196rpx;
  background: #F6F6F6;
  border-radius: 16rpx;
  margin: 20rpx auto;
}

.eyes-box{
  width: 638rpx;
  height: fit-content;
  box-sizing: border-box;
  /* background: #F6F6F6; */
  
  background-size: 100%;
  border-radius: 16rpx;
  padding:16rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
}
.eyeline{
  width: 1rpx;
  height: 147rpx;
  border-right: 1rpx dashed  #E8E8E2;
}
.eyes-img{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 251rpx;
  height: 160rpx;
  border-radius: 16rpx;
}

.eye-card{
  width:112rpx;
  height:38rpx;
  text-align: center;
  position: absolute;
  left: 5rpx;
  top: 5rpx;
  border-radius: 20rpx 0rpx 20rpx 0rpx;
  color: #fff;
  background: linear-gradient(228deg, #FC3DB8 0%, #F57F27 100%);
}
.eye-card-active{
  width:112rpx;
  height:38rpx;
  text-align: center;
  position: absolute;
  left: 5rpx;
  top: 5rpx;
  border-radius: 20rpx 0rpx 20rpx 0rpx;
  color: #FFFBF8;
  background: #727272;
}
.discountText{
  font-size: 25rpx;
  color: #FFFBF8;
}
.eyesbg{
  width: 187rpx;
  height: 85rpx;
  font-size: 62rpx;
  text-align: center;
  font-family: MiSans-Heavy, MiSans;
  font-weight: 800;
  color: #FB2F25;
  line-height: 96rpx;
}
.eyesbg-active{
  width: 122rpx;
  height: 76rpx;
  font-size: 47rpx;
  text-align: center;
  font-family: MiSans-Heavy, MiSans;
  font-weight: 800;
  color:#B2B2B2;
  line-height: 96rpx;

}
.eyeTitie{
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #E77C77;
  line-height: 28rpx;
}
.eyeTitie-active{
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #DBDAD8;
  line-height: 28rpx;
}

.eye-serve{
  width:500rpx;
  height: 174rpx;
  margin:20rpx 0rpx 0rpx 20rpx;
}
.eyes-title-active{
  font-size: 32rpx;
  font-weight: bold;
  color: #A9A9A9;
  -webkit-line-clamp: 1;				/* 文本位置只保留两行 */
  text-overflow: ellipsis;			/* 溢出文本省略号代替 */
  display: -webkit-box;				/*  */
  -webkit-box-orient: vertical;		/*  */
  overflow: hidden;	
}
.eyes-title{
  font-size: 32rpx;
  font-weight: bold;
  color: #FB2F25;
  -webkit-line-clamp: 1;				/* 文本位置只保留两行 */
  text-overflow: ellipsis;			/* 溢出文本省略号代替 */
  display: -webkit-box;				/*  */
  -webkit-box-orient: vertical;		/*  */
  overflow: hidden;					/* 溢出隐藏 */
}
.eyes-text{
  height:50rpx;
  margin-top:6rpx;
  font-size:24rpx;
  color:#E77C77;
  line-height: 35rpx;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  overflow: hidden;
}
.eyes-text-active{
  height:60rpx;
  margin-top:6rpx;
  font-size:24rpx;
  color:#CACBCA;
  line-height: 35rpx;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  overflow: hidden;
}
.eyes-count{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 168rpx;

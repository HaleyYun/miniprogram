/* packageHome/pages/confirmOrder/index.wxss */
.page{
  background: #F3F4F6;
  overflow-y: auto;
  margin-bottom: 150rpx;
}
.subscribe-person{
  width:686rpx;
  /* height:200rpx; */
  margin: 30rpx auto;
}
.top{
  width:680rpx;
  height:60rpx;
  display: flex;
  justify-content: space-between;
}
.title{
  font-size: 18px;
  font-weight: bold;
  color:#222;
}
.choose{
  width:165rpx;
  height:50rpx;
  line-height: 50rpx;
  border-radius: 20px;
  border:1px solid #fff;
  background: rgba(255,255,255,0.36);
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.09);
}
.text{
  width:110rpx;
  height: 50rpx;
  margin:auto;
  font-size: 13px;
  color: #222;
  display: flex;
}
.img{
  width: 30rpx;
  height: 35rpx;
  margin-top:10rpx;
}
.person{
  width:640rpx;
  height:80rpx;
  background-color: #fff;
  border-radius: 10px;
  margin-top:20rpx;
  padding:25rpx;
  display: flex;
  justify-content: space-between;
}
.person-img,.per-name{
  display: flex;
}
.headbg{
  width:84rpx;
  height:84rpx;
  border-radius: 5px;
  margin-right: 30rpx;
}
.bg{
  width:84rpx;
  height:84rpx;
}
.name{
  font-size: 16px;
  font-weight: bold;
  color:#222;
}
.box{
  width: 80rpx;
  height: 35rpx;
  margin:10rpx;
  color:#646363;
  font-size: 10px;
  text-align: center;
  line-height: 35rpx;
  background: #F3F4F6;
  border-radius: 16rpx 16rpx 16rpx 0rpx;
}
.phone1{
  font-size: 12px;
  color:#6e6e6e;
  line-height: 35rpx;
}
.person-btn{
  width: 96rpx;
  height: 52rpx;
  background: #F3F4F6;
  border-radius: 6rpx;
  text-align: center;
  color:#222;
  font-size: 12px;
  line-height: 52rpx;
  margin-top:18rpx;
}
.from{
  width: 686rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin: 20rpx auto;
}
.notice{
  height: 60rpx;
  background-color: #FFFAEB;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #CE963C;
  display: flex;
  align-items: center;
  border-radius: 16rpx 16rpx 0 0;
  padding: 0 30rpx;
}
.reminder{
  width: 28rpx;
  height: 28rpx;
}
.purchaser,
.phone{
  display: flex;
  padding: 0 30rpx;
  height: 82rpx;
  align-items: center;
}
.purchaser-title,
.phone-title{
  width: 150rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #222222;
}
.details{
  width: 686rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin: 0 auto;
  padding: 22rpx 28rpx 26rpx;
  box-sizing: border-box;
}
.details-title{
  font-size: 32rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #222222;
  margin-bottom: 30rpx;
}
.details-center{
  display: flex;
  justify-content: space-between;
}
.details-image{
  width: 172rpx;
  height: 172rpx;
  border-radius: 16rpx;
}
.details-content{
  width: 443rpx;
}
.content-title{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  font-weight: bold;
  color: #222222;
  margin-bottom: 10rpx;
  -webkit-line-clamp: 1;				/* 文本位置只保留两行 */
  text-overflow: ellipsis;			/* 溢出文本省略号代替 */
  display: -webkit-box;				/*  */
  -webkit-box-orient: vertical;		/*  */
  overflow: hidden;					/* 溢出隐藏 */
}
.content-title-price::before{
  content: '¥';
  font-size: 16rpx;
  font-weight: bold;
  color: #222222;
}
.introduce{
  font-size: 16rpx;
  -webkit-line-clamp: 3;				/* 文本位置只保留两行 */
  text-overflow: ellipsis;			/* 溢出文本省略号代替 */
  display: -webkit-box;				/*  */
  -webkit-box-orient: vertical;		/*  */
  overflow: hidden;					/* 溢出隐藏 */
  font-weight: 400;
  color: #222222;
}
.details-subtotal{
  height: 56rpx;
  display: flex;
  justify-content: flex-end;
  align-items: flex-end;
  font-size: 40rpx;
  font-weight: bold;
  color: #222222;
}
.details-subtotal::before{
  content: '小计：¥';
  font-size: 28rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #222222;
  margin-bottom: 5rpx;
}
.method{
  width: 686rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin:  20rpx auto;
  padding: 30rpx;
  box-sizing: border-box;
}
.methods{
  width: 686rpx;
  /* height: 400rpx; */
  background: #FFFFFF;
  border-radius: 16rpx;
  margin:  20rpx auto;
  padding: 30rpx;
  box-sizing: border-box;
}
.method-title{
  font-size: 32rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #222222;
  margin-bottom: 10rpx;
}
.method-list{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 40rpx;
}
.method-mode{
  display: flex;
  align-items: center;
}
.method-icon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.tick{
  display: flex;
  border-radius: 50%;
  align-items: center;
  justify-content: center;
}
.tick-deactive{
  width: 34rpx;
  height: 34rpx;
  border: 2rpx solid gray;
}
.tick-active{
  width: 36rpx;
  height: 36rpx;
  background-color: #FF5A00;
}
.tick-image{
  width: 22rpx;
  height: 22rpx;
}
.goodNot{
  color: #C2C4C6;
}
.payment{
  width: 100%;
  /* height: 168rpx; */
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: white;
  display: flex;
  align-items: center;
  padding: 36rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.payment-unit{
  display: flex;
  align-items: flex-end;
}
.payment-subtotal{
  font-size: 28rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #222222;
  margin-bottom: 10rpx;
}
.payment-price{
  font-size: 52rpx;
  font-family: Oswald-Medium, Oswald;
  font-weight: bold;
  color: #FF5A00;
}
.payment-price::before{
  content: '¥';
  font-size: 34rpx;
  font-weight: bold;
}
.payment-purchase{
  width: 252rpx;
  height: 78rpx;
  background-color: #3476FF;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 39rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #FFFFFF;
}
.phone-input{
  color: #000;
}
.purchaser-input{
  color: #000;
}
.amount{
  margin-top: 40rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
font-weight: 400;
color: #111111;
line-height: 40rpx;
}
.favorable{
  margin-top: 40rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  font-weight: 400;
  color: #111111;
  line-height: 40rpx;
}
.goodfavorable{
 display: flex;
 align-items: center;

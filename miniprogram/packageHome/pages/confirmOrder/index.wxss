font-family: MiSans-Bold, MiSans;
font-weight: bold;
color: #FB2F25;
/* line-height: 38rpx; */
}
.concent-title{
  display: flex;
}
.goodtotal{
  font-size: 34rpx;
  font-weight: bold;
  color: #FB2F25;
}
.goodamount{
  font-size: 34rpx;
  font-weight: bold;
  color: rgba(34,34,34,0.92);
}
.goodicon{
  width: 28rpx;
  height: 28rpx;
  font-size: 28rpx;
font-family: MiSans-Bold, MiSans;
font-weight: bold;
color: rgba(34,34,34,0.92);
line-height: 38rpx;
}
.total{
  margin-top: 40rpx;
  display: flex;
  justify-content: space-between;
}




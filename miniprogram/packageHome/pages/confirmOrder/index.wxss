  .bottom-dialog-body{width:100%; position:absolute; z-index:3000; bottom:0; left:0;background:#fff;}
  /*动画前初始位置*/
  .bottom-positon{-webkit-transform:translateY(100%);transform:translateY(100%);}
   
   

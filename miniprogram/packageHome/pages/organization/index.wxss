}

.placeholderStyle { 
  color: #333; 
}


.choose-address{
  width:686rpx;
  height:50rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
}
.textOne{
  font-size: 14px;
  color:#222;
  font-weight: bold;
}
.picker{
  width:686rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;

  background-color: white;
  height: fit-content;
  padding:28rpx;
  overflow: hidden;
  border-radius: 32rpx 0 16rpx 16rpx ;
  position: relative;
  z-index: 9;
}
.distanced{
  display: flex;
  justify-content: space-between;
}
.distance{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ABABAB;
}
.count{
  width: 284rpx;
  height: 60rpx;
  display: flex;
  border-radius: 30rpx 30rpx 30rpx 0rpx;
}
.blue-box{
  width: 122rpx;
  height: 60rpx;
  background: #3476FF;
  color:#fff;
  font-size: 23px;
  font-weight: bold;
  text-align: center;
  line-height: 60rpx;
  border-radius: 30rpx 0rpx 30rpx 0rpx
}
.blue-text{
  font-size: 13px;
  color:#3476FF;
  font-weight: bold;
  line-height: 60rpx;
  text-indent: 20rpx;
}
.cont-title{
  width: 648rpx;
  height: 50rpx;
  font-size: 18px;
  font-weight: bold;
  color:#222;
  line-height: 60rpx;
}
.tags-list{
  display: flex;
  margin-top: 20rpx;
}
.tags-org{
  padding: 2rpx 12rpx;
  border-radius: 4rpx;
  font-size: 20rpx;
  margin: 0 10rpx;
}
.tags-org:nth-child(3n-2){
  background: #FCEFDF;
  color: #714721;
}
.tags-org:nth-child(3n-1){
  background: #EEF4F4;
  color: #33757D;
}
.tags-org:nth-child(3n){
  background: #EDEDFC;
  color: #3C5BCE;
}
.cont-remark{
  display: flex;
  height:50rpx;
}
.top20{
  margin-top: 20rpx;
}
.label{
  height:40rpx;
  line-height: 40rpx;
  color:#555;
  font-size: 12px;
}
.remarks{
  width:500rpx;
  height:40rpx;
  line-height: 40rpx;
  font-size: 12px;
  color:#222;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1; 
  overflow: hidden;
}
.time{
  font-size: 12px;
  font-weight: bold;
  color: #3476FF;
  line-height: 40rpx;
}
.address{
  width:500rpx;
  height:fit-content;
  font-size: 12px;
  color:#222;
  line-height: 40rpx;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  overflow: hidden;
}

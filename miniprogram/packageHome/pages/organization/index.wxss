}
.doctor-box{
  width: fit-content;
  margin: 0 auto;
  margin-top: 200rpx;
  display: flex;
  flex-direction: column;
}
.doctortext{
  margin-top: 50rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #666666;
  text-align: center;
}
.doctor{
  width: 378rpx;
  height: 320rpx;
  margin: 0 auto;
  transform: translate(-60rpx);
}

.photo-content {
  width: 100%;
  position: fixed;
  height: 100vh;
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

.video-box {
  width: 750rpx;
  height: 100vh;
}

.check-number {
  width: 100%;
}

.num-count {
  width: 100%;
  height: 400rpx;
  font-size: 76rpx;
  color: #8B4C3D;
  text-align: center;
  line-height: 400rpx;
  font-weight: bold;
}

.con-start {
  width: 470rpx;
  height: 180rpx;
  margin: 200rpx auto;
}

.con-start image {
  width: 470rpx;
  height: 180rpx;
}

.con-start {
  position: relative;
  animation: myanimate 5s infinite;
  -webkit-animation: myanimate 5s infinite;
  animation-direction: alternate;
  animation-timing-function: ease-in-out;
  -webkit-animation: myanimate 5s infinite;
  -webkit-animation-direction: alternate;
  -webkit-animation-timing-function: ease-in-out;
}

@keyframes myanimate {
  0% {
    transform: scale(1);
  }

  25% {
    transform: scale(1.2);
  }

  50% {
    transform: scale(1);
  }

  75% {
    transform: scale(1.2);
  }

}

@-webkit-keyframes myanimate {
  0% {
    transform: scale(1);
  }

  25% {
    transform: scale(1.2);
  }

  50% {
    transform: scale(1);
  }

  75% {
    transform: scale(1.2);
  }
}

.bigDiv {
  /* width: 100%; */
  height: 100%;
  padding: 40rpx;
  margin-top: 160rpx;
}

.number-box {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}

.numberlist-box {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.list-content {
  width: 130rpx;
  height: 130rpx;
  line-height: 130rpx;
  text-align: center;
  font-size: 60rpx;
  font-weight: bold;
  color: #ffffff;
  border-radius: 50%;
  background-image: url(https://neuroweave.oss-cn-hangzhou.aliyuncs.com/zskj-app-test/games/images/red.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-bottom: 10px;
  animation: circleD ease-in-out 0s 1 alternate forwards;
  -webkit-animation: circleD 2s ease-in-out 0s 1 alternate forwards;
}

@keyframes circleD {
  0% {
    transform: scale(1);
  }

  25% {
    transform: scale(1.1);
  }

  50% {
    transform: scale(1.2);
  }

}

@-webkit-keyframes circleD {
  0% {
    transform: scale(1);
  }

  25% {
    transform: scale(1.1);
  }

  50% {
    transform: scale(1.2);
  }
}

.even-bg {
  /* background-image: url(../../static/images/blue.png); */
  background-image: url(https://neuroweave.oss-cn-hangzhou.aliyuncs.com/zskj-app-test/games/images/blue.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.ishidebg {
  visibility: hidden;
}

.doctor {
  display: flex;
  width: 100%;
  position: fixed;
  bottom: 100rpx;
}

.doctor-bg {
  width: 100%;
  height: 320rpx;
  margin-top: 40rpx;
}

.doctor-bg image {
  margin-top: 30rpx;
  margin-left: 100rpx;
  width: 212rpx;
  height: 312rpx;
}

.doctor-text {
  width: 400rpx;
  height: 198rpox;
  border-radius: 50rpx;
  margin-top: 100rpx;
  margin-left: 30rpx;
  padding: 24rpx;
  text-align: left;
  background: linear-gradient(133deg, #FFF7EA 0%, #FFF7EA 100%);
  box-shadow: 0 4rpx 6rpx 0 rgba(0, 0, 0, 0.12), inset 0 -6rpx 6rpx 2rpx #E1CEBB;
  border: 6rpx solid #968273;
  font-size: 32rpx;
  color: #7D4742;
  line-height: 44rpx;
  letter-spacing: 2rpx;
  position: relative;
  z-index: 0;
}

.doctor-text:before {
  border-style: solid;
  border-width: 20rpx 0 20rpx 20rpx;
  border-color: transparent transparent transparent #FFF7EA;
  content: "";
  position: absolute;
  top: 50%;
  right: -16rpx;
  margin-top: -18rpx;
  display: block;
  width: 0;
  height: 0;
  z-index: 0;
}

.doctor-text:after {
  border-style: solid;
  border-width: 22rpx 0 22rpx 22rpx;
  border-color: transparent transparent transparent #968273;
  content: "";
  position: absolute;
  top: 50%;
  right: -24rpx;
  margin-top: -20rpx;
  display: block;
  width: 0;
  height: 0;
  z-index: -1;
}
.doctor-text {
  border-radius: 50rpx;
  margin-top: 100rpx;
  margin-left: 30rpx;
  padding: 24rpx;
  text-align: left;
  background: linear-gradient(133deg, #FFF7EA 0%, #FFF7EA 100%);
  box-shadow: 0 4rpx 6rpx 0 rgba(0, 0, 0, 0.12), inset 0px -6rpx 6rpx 2rpx #E1CEBB;
  border: 6rpx solid #968273;
  font-size: 32rpx;
  color: #7D4742;
  line-height: 44rpx;
  letter-spacing: 2rpx;
  position: relative;
  z-index: 0;
}

.doctor-text:before {
  border-style: solid;
  border-width: 20rpx 0 20rpx 20rpx;
  border-color: transparent transparent transparent #FFF7EA;
  content: "";
  position: absolute;
  top: 50%;
  right: -16rpx;
  margin-top: -18rpx;
  display: block;
  width: 0;
  height: 0;
  z-index: 0;
}

.doctor-text:after {
  border-style: solid;
  border-width: 22rpx 0 22rpx 22rpx;
  border-color: transparent transparent transparent #968273;
  content: "";
  position: absolute;
  top: 50%;
  right: -24rpx;
  margin-top: -20rpx;
  display: block;
  width: 0;
  height: 0;
  z-index: -1;
}

